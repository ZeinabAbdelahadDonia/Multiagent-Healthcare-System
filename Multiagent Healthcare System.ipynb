    "        return Response()\n",
    "\n",
    "# Initialize model (replace with real model instance)\n",
    "ggmodel = MockModel()\n",
    "\n",
    "# Cell 3: Define ScansAgentApp\n",
    "class ScansAgentApp:\n",
    "    def __init__(self, root):\n",
    "        self.root = root\n",
    "        self.root.title(\"X-ray Image Analysis\")\n",
    "        self.root.geometry(\"600x400\")\n",
    "\n",
    "        # Initialize variables\n",
    "        self.image_path = None\n",
    "        self.image_label = None\n",
    "        self.result_text = None  # Initialize result_text\n",
    "        self.analysis_result = \"No analysis performed\"  # Store result for return\n",
    "\n",
    "        # Handle window close event\n",
    "        self.root.protocol(\"WM_DELETE_WINDOW\", self.on_closing)\n",
    "\n",
    "        # Create UI elements\n",
    "        self.create_ui()\n",
    "\n",
    "    def create_ui(self):\n",
    "        # Upload button\n",
    "        self.upload_button = tk.Button(\n",
    "            self.root, text=\"Upload X-ray Image\", command=self.upload_image\n",
    "        )\n",
    "        self.upload_button.pack(pady=10)\n",
    "\n",
    "        # Image display area\n",
    "        self.image_label = tk.Label(self.root, text=\"No image selected\")\n",
    "        self.image_label.pack(pady=10)\n",
    "\n",
    "        # Analyze button\n",
    "        self.analyze_button = tk.Button(\n",
    "            self.root, text=\"Analyze Image\", command=self.analyze_image, state=\"disabled\"\n",
    "        )\n",
    "        self.analyze_button.pack(pady=10)\n",
    "\n",
    "        # Result text area\n",
    "        self.result_text = tk.Text(self.root, height=40, width=160)\n",
    "        self.result_text.pack(pady=10)\n",
    "        self.result_text.insert(tk.END, \"Analysis results will appear here...\")\n",
    "        self.result_text.config(state=\"disabled\")\n",
    "\n",
    "    def upload_image(self):\n",
    "        # Open file picker dialog\n",
    "        self.image_path = filedialog.askopenfilename(\n",
    "            title=\"Select X-ray Image\",\n",
    "            filetypes=[(\"Image files\", \"*.jpg *.jpeg *.png\")]\n",
    "        )\n",
    "\n",
    "        if self.image_path:\n",
    "            try:\n",
    "                # Load and display the image\n",
    "                img = Image.open(self.image_path)\n",
    "                img = img.resize((200, 200), Image.LANCZOS)  # Resize for display\n",
    "                img_tk = ImageTk.PhotoImage(img)\n",
    "                \n",
    "                # Update image label\n",
    "                self.image_label.config(image=img_tk, text=\"\")\n",
    "                self.image_label.image = img_tk  # Keep reference\n",
    "\n",
    "                # Enable analyze button\n",
    "                self.analyze_button.config(state=\"normal\")\n",
    "            except Exception as e:\n",
    "                messagebox.showerror(\"Error\", f\"Failed to load image: {str(e)}\")\n",
    "                self.image_path = None\n",
    "                self.analyze_button.config(state=\"disabled\")\n",
    "        else:\n",
    "            self.image_label.config(image=\"\", text=\"No image selected\")\n",
    "            self.analyze_button.config(state=\"disabled\")\n",
    "\n",
    "    def analyze_image(self):\n",
    "        if not self.image_path:\n",
    "            messagebox.showerror(\"Error\", \"No image selected for analysis\")\n",
    "            return\n",
    "            \n",
    "        # Define the radiologist prompt\n",
    "        prompt = (\n",
    "            \"You are a radiologist with expertise in interpreting X-ray images. \"\n",
    "            \"Analyze the provided X-ray image and provide a detailed description of the findings. \"\n",
    "            \"Include observations about bone structures, soft tissues (if visible), any abnormalities, \"\n",
    "            \"and your overall impression.\"\n",
    "        )\n",
    "\n",
    "        try:\n",
    "            # Read the image file\n",
    "            img = Image.open(self.image_path)\n",
    "\n",
    "            # Prepare content for the model\n",
    "            contents = [prompt, img]\n",
    "            response = ggmodel.generate_contents(contents)\n",
    "            analysis = response.text\n",
    "\n",
    "            # Update result text and store result\n",
    "            self.result_text.config(state=\"normal\")\n",
    "            self.result_text.delete(1.0, tk.END)\n",
    "            self.result_text.insert(tk.END, analysis)\n",
    "            self.result_text.config(state=\"disabled\")\n",
    "            self.analysis_result = analysis\n",
    "\n",
    "        except Exception as e:\n",
    "            analysis = f\"Error processing image: {str(e)}\"\n",
    "            self.result_text.config(state=\"normal\")\n",
    "            self.result_text.delete(1.0, tk.END)\n",
    "            self.result_text.insert(tk.END, analysis)\n",
    "            self.result_text.config(state=\"disabled\")\n",
    "            self.analysis_result = analysis\n",
    "\n",
    "    def on_closing(self):\n",
    "        # Save current result text before closing\n",
    "        if self.result_text:\n",
    "            self.analysis_result = self.result_text.get(1.0, tk.END).strip()\n",
    "        # Destroy the root window\n",
    "        self.root.destroy()\n",
    "\n",
    "# Cell 4: Define scans_agent for LangGraph integration\n",
    "def scans_agent(state: Dict[str, Any]) -> Dict[str, Any]:\n",
    "    # Initialize tkinter app\n",
    "    root = tk.Tk()\n",
    "    app = ScansAgentApp(root)\n",
    "    try:\n",
    "        root.mainloop()\n",
    "    except KeyboardInterrupt:\n",
    "        # Handle manual interruption gracefully\n",
    "        app.on_closing()\n",
    "\n",
    "    # Return the analysis result for LangGraph\n",
    "    chat_history = state[\"chat_history\"]\n",
    "    chat_history.append({\"role\": \"system\", \"content\": f\"X-ray analysis: {app.analysis_result}\"})\n",
    "    return {\"scans_results\": app.analysis_result, \"chat_history\": chat_history}\n",
    "\n",
    "# Cell 5: Example usage in Jupyter (for testing)\n",
    "if __name__ == \"__main__\":\n",
    "    # Simulate LangGraph state\n",
    "    test_state = {\n",
    "        \"user_input\": \"I have an X-ray showing chest pain.\",\n",
    "        \"chat_history\": []\n",
    "    }\n",
    "    result = scans_agent(test_state)\n",
    "    print(result)"
