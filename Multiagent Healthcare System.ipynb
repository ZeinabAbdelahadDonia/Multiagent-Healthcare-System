    "import requests\n",
    "from xml.etree import ElementTree\n",
    "\n",
    "@tool\n",
    "def search_pubmed(query: str, max_results: int = 5) -> str:\n",
    "    \"\"\"\n",
    "    Search PubMed for medical literature related to the query using the NCBI E-Utilities API.\n",
    "\n",
    "    Args:\n",
    "        query: The search query for PubMed.\n",
    "        max_results: Number of articles to fetch.\n",
    "    \n",
    "    Returns:\n",
    "        A string summary of abstracts from relevant articles.\n",
    "    \"\"\"\n",
    "    # Step 1: Use ESearch to get PubMed IDs\n",
    "    esearch_url = \"https://eutils.ncbi.nlm.nih.gov/entrez/eutils/esearch.fcgi\"\n",
    "    esearch_params = {\n",
    "        \"db\": \"pubmed\",\n",
    "        \"term\": query,\n",
    "        \"retmax\": max_results,\n",
    "        \"retmode\": \"json\"\n",
    "    }\n",
    "    esearch_resp = requests.get(esearch_url, params=esearch_params)\n",
    "    id_list = esearch_resp.json().get(\"esearchresult\", {}).get(\"idlist\", [])\n",
    "\n",
    "    if not id_list:\n",
    "        return f\"No results found in PubMed for query: '{query}'\"\n",
    "\n",
    "    # Step 2: Use EFetch to get article summaries\n",
    "    efetch_url = \"https://eutils.ncbi.nlm.nih.gov/entrez/eutils/efetch.fcgi\"\n",
    "    efetch_params = {\n",
    "        \"db\": \"pubmed\",\n",
    "        \"id\": \",\".join(id_list),\n",
    "        \"retmode\": \"xml\"\n",
    "    }\n",
    "    efetch_resp = requests.get(efetch_url, params=efetch_params)\n",
    "\n",
    "    root = ElementTree.fromstring(efetch_resp.content)\n",
    "    summaries = []\n",
    "\n",
    "    for article in root.findall(\".//PubmedArticle\"):\n",
    "        try:\n",
    "            title = article.find(\".//ArticleTitle\").text\n",
    "            abstract = article.find(\".//Abstract/AbstractText\").text\n",
    "            summaries.append(f\"Title: {title}\\nAbstract: {abstract}\")\n",
    "        except Exception:\n",
    "            continue\n",
    "\n",
    "    return \"\\n\\n---\\n\\n\".join(summaries) if summaries else \"No abstracts found.\"\n"
   ]
