    "from langgraph.graph import StateGraph, END\n",
    "\n",
    "# Initialize the graph\n",
    "workflow = StateGraph(AgentState)\n",
    "\n",
    "# Add nodes for each agent\n",
    "workflow.add_node(\"abbreviation_disambiguation\", abbreviation_disambiguation_agent)\n",
    "workflow.add_node(\"symptom_analysis\", symptom_analysis_agent)\n",
    "workflow.add_node(\"drug_advisor\", drug_advisor_agent)\n",
    "workflow.add_node(\"pubmed\", researcher_node)\n",
    "workflow.add_node(\"diagnostic_summary\", diagnostic_summary_agent)\n",
    "workflow.add_node(\"evaluationn\", evaluation_agent)  # Fixed typo\n",
    "workflow.add_node(\"router\", router_agent)\n",
    "workflow.add_node(\"scans_agent\", scans_agent)\n",
    "workflow.add_node(\"prescription_agent\", prescription_agent)\n",
    "workflow.add_node(\"drug_agent\", drug_agent)\n",
    "\n",
    "# Router logic\n",
    "workflow.set_entry_point(\"router\")\n",
    "\n",
    "# Conditional edges from router\n",
    "def router_decision(state):\n",
    "    strDes = router_agent(state)[\"route\"]\n",
    "    return strDes\n",
    "\n",
    "workflow.add_conditional_edges(\n",
    "    \"router\",\n",
    "    router_decision,\n",
    "    {\n",
    "        \"abbreviation_disambiguation\": \"abbreviation_disambiguation\",\n",
    "        \"scans_agent\": \"scans_agent\",\n",
    "        \"prescription_agent\": \"prescription_agent\",\n",
    "        \"drug_agent\": \"drug_agent\",\n",
    "    }\n",
    ")\n",
    "\n",
    "# Sequential edges for abbreviation_disambiguation path\n",
    "workflow.add_edge(\"abbreviation_disambiguation\", \"symptom_analysis\")\n",
    "workflow.add_edge(\"symptom_analysis\", \"pubmed\")\n",
    "workflow.add_edge(\"pubmed\", \"drug_advisor\")\n",
    "workflow.add_edge(\"drug_advisor\", \"diagnostic_summary\")\n",
    "workflow.add_edge(\"diagnostic_summary\", \"evaluationn\")\n",
    "workflow.add_edge(\"evaluationn\", END)\n",
    "\n",
    "# Edges for other agents (assuming they are terminal for now)\n",
    "workflow.add_edge(\"scans_agent\", END)\n",
    "workflow.add_edge(\"prescription_agent\", END)\n",
    "workflow.add_edge(\"drug_agent\", END)\n",
    "\n",
    "# Compile the workflow\n",
    "workflow.compile()"
   ]
